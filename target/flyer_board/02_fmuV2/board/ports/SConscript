import os
from building import *

cwd  = GetCurrentDir()
path =  [cwd]
src  = []

if GetDepend(['BSP_USING_ON_CHIP_FLASH']):
    src += Glob('fal_flash_stm32f4_port.c')

CPPDEFINES = ['STM32F427xx']
group = DefineGroup('Drivers', src, depend = [''], CPPPATH = path, CPPDEFINES = CPPDEFINES)

Return('group')
